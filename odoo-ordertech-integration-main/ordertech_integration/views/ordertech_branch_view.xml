<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="ordertech_branch_action" model="ir.actions.act_window">
        <field name="name">Branches</field>
        <field name="res_model">res.company</field>
        <field name="view_mode">list,form</field>
        <field name="domain">[('is_branch','=',True),('ordertech_tenant_branchId','!=',False)]</field>
        <field name="context">{'create':False}</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                No Synced Branch Found!
            </p>
        </field>
    </record>

    <menuitem id="ordertech_branch_menu"
              name="Branch"
              sequence="2"
              parent="ordertech_restaurant_menu_root"
              action="ordertech_branch_action"/>
</odoo>