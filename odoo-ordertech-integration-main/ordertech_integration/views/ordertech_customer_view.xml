<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="ordertech_customer_action" model="ir.actions.act_window">
        <field name="name">Customers</field>
        <field name="res_model">res.partner</field>
        <field name="view_mode">list,form</field>
        <field name="domain">[('customer_rank','>',0),('ordertech_customerId','!=',False)]</field>
        <field name="context">{'create':False}</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                No Synced Customers Found!
            </p>
        </field>
    </record>

    <menuitem id="ordertech_customer_menu_root"
              name="Customers"
              sequence="2"
              parent="ordertech_root"/>
    <menuitem id="ordertech_customer_menu"
              name="Customer"
              sequence="1"
              parent="ordertech_customer_menu_root"
              action="ordertech_customer_action"/>
</odoo>