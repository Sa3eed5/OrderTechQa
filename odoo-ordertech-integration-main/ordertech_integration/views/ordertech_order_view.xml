<?xml version="1.0" encoding="utf-8" ?>
<odoo>
        <record id="ordertech_order_action" model="ir.actions.act_window">
        <field name="name">OrderTech Orders</field>
        <field name="res_model">pos.order</field>
        <field name="view_mode">list,form</field>
        <field name="domain">[('ordertech_orderId','!=',False)]</field>
        <field name="context">{'create':False,'edit':False}</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                No Synced Orders Found!
            </p>
        </field>
    </record>

    <menuitem id="ordertech_order_menu_root"
              name="Orders"
              sequence="4"
              parent="ordertech_root"/>
    <menuitem id="ordertech_order_menu"
              name="Order"
              sequence="1"
              parent="ordertech_order_menu_root"
              action="ordertech_order_action"/>
</odoo>