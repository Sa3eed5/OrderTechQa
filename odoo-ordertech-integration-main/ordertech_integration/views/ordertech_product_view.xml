<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="ordertech_menu_product_action" model="ir.actions.act_window">
        <field name="name">Products</field>
        <field name="res_model">product.template</field>
        <field name="view_mode">list,form</field>
        <field name="domain">[('ordertech_productId','!=',False)]</field>
        <field name="context">{'create':False}</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                No Synced Product Found!
            </p>
        </field>
    </record>

    <menuitem id="ordertech_menu_products_menu"
              name="Products"
              sequence="2"
              parent="ordertech_menu_menu_root"
              action="ordertech_menu_product_action"/>
</odoo>